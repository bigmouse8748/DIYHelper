{
    "family": "cheasydiy-production-backend",
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::571600828655:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "backend",
            "image": "571600828655.dkr.ecr.us-east-1.amazonaws.com/cheasydiy/backend:5518714869043e4f66d1c10c1b7d18c16c342f80",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8001,
                    "hostPort": 8001,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/cheasydiy-production",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "backend"
                }
            },
            "environment": [
                {
                    "name": "DATABASE_URL",
                    "value": "postgresql://dbadmin:ChEasyDiy2024!@cheasydiy-production-db.c9sieeomsxup.us-east-1.rds.amazonaws.com:5432/cheasydiy"
                },
                {
                    "name": "ENVIRONMENT",
                    "value": "production"
                },
                {
                    "name": "AWS_REGION",
                    "value": "us-east-1"
                }
            ]
        }
    ]
}